@{
    Layout = "~/Views/Shared/_AdminLayout1.cshtml";
}
@{
    ViewData["Title"] = "Admin: Colore";
}
@model IQueryable<WebApplication7.Models.AllHistory>
<section class="content">
    <div class="row mr-0">
        <div class="col-12">
            <div class="card">
                <div class="card-header">
                    <h3 class="card-title">Colore Table</h3>
                </div>
                <!-- /.card-header -->
                <div class="card-body">
                    <table id="example2" class="table table-bordered table-hover">
                        <thead>
                            <tr>
                                <th>UserName</th>
                                <th>Product</th>
                                <th>Price</th>
                                <th>Quantity</th>
                                <th>Time</th>
                                <th>DeliveryAddress</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {
                            <tr>
                                <td>@item.UserName</td>
                                <td>@item.Name</td>
                                <td>@item.Price</td>
                                <td>@item.Quantity</td>
                                <td>@item.dateTime</td>
                                <td>@item.DeliveryAddress</td>

                            </tr>
                            }
                        </tbody>
                        <tfoot>
                            <tr>
                                <th>UserName</th>
                                <th>Product</th>
                                <th>Price</th>
                                <th>Quantity</th>
                                <th>Time</th>
                                <th>DeliveryAddress</th>
                            </tr>
                        </tfoot>
                    </table>
                </div>
                <!-- /.card-body -->
            </div>
            <!-- /.card -->


        </div>
        <!-- /.card -->
    </div>
    <!-- /.row -->
</section>

